{
  "compilerOptions": {
    "lib": [
      "es2015",
      "es2017",
      "dom"
    ],
    "baseUrl": "./",
    "declaration": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "../dist",
    "sourceMap": true,
    "skipLibCheck": true,
    "strict": true,
    "types": [
      "./pixel/pixel"
    ]
  },
  "exclude": [
    "../dist"
  ]
}
